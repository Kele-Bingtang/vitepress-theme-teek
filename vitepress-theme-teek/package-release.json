{
  "name": "vitepress-theme-teek",
  "version": "1.0.0",
  "description": "vitepress-theme-teek",
  "type": "module",
  "author": {
    "name": "teeker",
    "email": "2456019588@qq.com",
    "url": "https://tianke99.cn"
  },
  "keywords": ["vitepress", "theme", "markdown", "vue", "ui", "blog"],
  "license": "MIT",
  "homepage": "https://github.com/Kele-Bingtang/vitepress-theme-teek/tree/master",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kele-Bingtang/vitepress-theme-teek.git"
  },
  "bugs": {
    "url": "https://github.com/Kele-Bingtang/vitepress-theme-teek/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "style": "index.css",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": ["./es/*.d.ts", "./es/*/index.d.ts"],
      "import": "./es/*.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": ["./lib/*.d.ts", "./lib/*/index.d.ts"],
      "require": "./lib/*.js"
    },
    "./config": {
      "types": "./es/config/index.d.ts",
      "import": "./es/config/index.mjs"
    },
    "./vp-plus/*.scss": "./theme-chalk/src/vp-plus/*.scss",
    "./tk-plus/*.scss": "./theme-chalk/src/tk-plus/*.scss",
    "./*": "./*"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "markdown-it-container": "^4.0.0",
    "vitepress-plugin-auto-frontmatter": "^1.0.7",
    "vitepress-plugin-catalogue": "^1.0.9",
    "vitepress-plugin-doc-analysis": "^1.0.10",
    "vitepress-plugin-file-content-loader": "^1.0.9",
    "vitepress-plugin-md-h1": "^1.0.8",
    "vitepress-plugin-permalink": "^1.0.17",
    "vitepress-plugin-sidebar-resolve": "^1.0.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "sass": "^1.83.1",
    "unbuild": "^3.2.0",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vitepress": "^1.6.3"
  },
  "browserslist": ["> 1%", "not ie 11", "not op_mini all"]
}
